tion,totalprice} = props;\nconst dispatch = useDispatch()\nconst addItem=()=>{\n  dispatch({type:'ADD ITEM',receiveItem:{\n      totalprice:price,\n      price:price,\n      title:title,\n      description:description,\n      id:id,\n      quantity:1\n  }})\n}\nconst removeItem =()=>{\n  dispatch({type:'REMOVE ITEM',receiveItem:{\n    totalprice:price,\n    price:price,\n    title:title,\n    description:description,\n    id:id,\n    quantity:1\n  }})\n}\n  return (\n    <li className={classes.item}>\n      <header>\n        <h3>{title}</h3>\n        <div className={classes.price}>\n          ${totalprice.toFixed(2)}{' '}\n          <span className={classes.itemprice}>(${price.toFixed(2)}/item)</span>\n        </div>\n      </header>\n      <div className={classes.details}>\n        <div className={classes.quantity}>\n          x <span>{quantity}</span>\n        </div>\n        <div className={classes.actions}>\n          <button type='button' onClick={removeItem}>-</button>\n          <button type='button' onClick={addItem}>+</button>\n        </div>\n      </div>\n    </li>\n  );\n};\n\nexport default CartItem;\n","C:\\Users\\FARID\\PROQRAMIROVANIYE\\tests\\react-complete-guide-code-19-advanced-redux\\code\\01-starting-project\\src\\components\\UI\\Card.js",[],"C:\\Users\\FARID\\PROQRAMIROVANIYE\\tests\\react-complete-guide-code-19-advanced-redux\\code\\01-starting-project\\src\\components\\Cart\\CartButton.js",[],"C:\\Users\\FARID\\PROQRAMIROVANIYE\\tests\\react-complete-guide-code-19-advanced-redux\\code\\01-starting-project\\src\\components\\store\\cartShowReducer.js",[],"C:\\Users\\FARID\\PROQRAMIROVANIYE\\tests\\react-complete-guide-code-19-advanced-redux\\code\\01-starting-project\\src\\components\\store\\cartReduucer.js",["56"],"import cartShowReducer from \"./cartShowReducer\"\r\n\r\n\r\n\r\nconst initialState = {\r\n    items:[],\r\n    totalQuantity:0\r\n}\r\n\r\n\r\nconst cartReducer =(state=initialState,action)=>{\r\n    if(action.type === 'ADD ITEM'){\r\n     const total = state.totalQuantity + action.receiveItem.quantity  \r\n     let updateItems = []\r\n    const index = state.items.findIndex(item=>item.id === action.receiveItem.id)\r\n    const itemIndex = state.items[index] \r\n     if(itemIndex){\r\n        const newItems =[...state.items]\r\n        const newItem = {...itemIndex,totalprice:itemIndex.totalprice + action.receiveItem.totalprice,quantity:itemIndex.quantity + action.receiveItem.quantity}\r\n        newItems[index]=newItem\r\n         updateItems=newItems\r\n     }else{\r\n         updateItems=state.items.concat(action.receiveItem)\r\n     }\r\n     return {items:updateItems,totalQuantity:total}\r\n     }\r\n     if(action.type === 'REMOVE ITEM'){\r\n        const total = state.totalQuantity - action.receiveItem.quantity  \r\n        let updateItems = []\r\n       const index = state.items.findIndex(item=>item.id === action.receiveItem.id)\r\n       const itemIndex = state.items[index] \r\n        if(itemIndex && itemIndex.quantity >1){\r\n           const newItems =[...state.items]\r\n           const newItem = {...itemIndex,totalprice:itemIndex.totalprice - action.receiveItem.totalprice,quantity:itemIndex.quantity - action.receiveItem.quantity}\r\n           newItems[index]=newItem\r\n            updateItems=newItems\r\n        }else if(itemIndex.quantity === 1){\r\n            updateItems=state.items.filter(item=>item.id !== action.receiveItem.id)\r\n        }\r\n        return {items:updateItems,totalQuantity:total}   \r\n     }\r\n    return state\r\n}\r\n\r\nexport default cartReducer",{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":2,"column":9,"nodeType":"63","messageId":"64","endLine":2,"endColumn":20},{"ruleId":"61","severity":1,"message":"65","line":1,"column":8,"nodeType":"63","messageId":"64","endLine":1,"endColumn":23},"no-native-reassign",["66"],"no-negated-in-lhs",["67"],"no-unused-vars","'useSelector' is defin